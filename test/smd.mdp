
integrator			= md

define				= -DDISRES_SUBDOMAIN

constraints			= all-bonds
nsteps				= 500000	; 1000ps
dt					= 0.002

nstxout				= 0		; xyz	
nstvout				= 0		; v		
nstfout				= 0		; F		
nstenergy			= 500	; E		1ps
nstxout-compressed	= 500	; xtc	1ps
compressed-x-grps	= Protein

nstlist				= 0
rlist				= 0

coulombtype			= cut-off
rcoulomb			= 0
cutoff-scheme		= group

vdw-type			= cut-off
rvdw 				= 0
DispCorr			= no

Tcoupl 				= nose-hoover
tc-grps 			= Protein
tau-t				= 1.0
ref-t				= 309.00
nsttcouple			= 10

energygrps			= Protein

Pcoupl				= no
gen_vel				= no 
pbc = no




pull				= yes
pull-nstxout		= 50		; pull-x	0.1ps
pull-nstfout		= 50		; pull-f	0.1ps

pull-ngroups		= 2
pull-group1_name	= b1-h1-b2
pull-group2_name	= h2-h3

pull-ncoords		= 1

pull-coord1-groups	= 1 2
pull-coord1-type	= umbrella
pull-coord1-start	= yes
pull-coord1-geometry= distance
pull-coord1-dim	= Y Y Y
pull-coord1-rate	= 0.02000 ; nm/ps *= 10000 A/ns ;           200 A/ns
pull-coord1-k		= 1000.1  ; kJ.mol-1.nm-2 *= 0.166  pN/A ;  166 pN/A

comm_mode = angular
disre = simple
